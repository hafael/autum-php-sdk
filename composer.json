{
    "name": "hafael/autum-php-sdk",
    "description": "Autum PHP SDK",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "keywords": ["autum", "php", "sdk", "laravel",  "api"],
    "require": {
        "php" : ">=5.6",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.5"
    },
    "authors": [
        {
            "name": "Rafael Villa Verde",
            "email": "rafael@autum.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "Autum\\SDK\\": ["src", "test"]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Autum\\SDK\\AutumSDKServiceProvider"
            ]
        }
    }
}
